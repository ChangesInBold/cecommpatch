<container type="vertbox" hsp="expand" vsp="expand" name="exterior_work_party_dialog_container" >
  
	<!-- Unassigned workers container -->
	<container type="horizbox" hsp="fixed" fixed_width="350" hap="center" vsp="fixed" fixed_height="75" vertical_padding="6" horizontal_padding="4" solidfill="1" solidfillname="cecommTopGlass"> 
		<container type="horizbox" hsp="minimum" vsp="expand" hap="center" horizontal_padding="4" vertical_padding="4" name="unassignedWorkersExteriorGlassPane">
			<container type="horizbox" hsp="minimum" vsp="expand" name="unassigned_workers" solidfillname="cecommPaperRoundInside" solidfill="1" horizontal_padding="8" vertical_padding="10" />
			
		</container>
		<container type="vertbox" hsp="minimum" vsp="minimum" hap="right" horizontal_padding="4" vertical_padding="4" vertical_gutter="2" name="helpBox"/>
	</container>
	<!-- Two pane worker panel and work group panel-->
	
	<container type="vertbox" hsp="expand" vsp="expand" solidfill="1" solidfillname="cecommWorkcrew" vertical_padding="6" vertical_gutter="3" horizontal_padding="4">
		<container type="scrollablevertbox"
				 name="work_parties"
				 hsp="expand" vsp="expand"
				 horizontal_padding="4" vertical_padding="4"
				 wheelScrollSpeed="60">

		</container>
	</container>
	
	<container type="horizbox" hsp="fixed" fixed_width="350" hap="center" vsp="fixed" fixed_height="20" vertical_padding="6" vertical_gutter="3" horizontal_padding="4" solidfill="1" solidfillname="cecommBottomGlass" />
</container>
