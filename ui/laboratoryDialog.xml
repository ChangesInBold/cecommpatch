			</container>

			<container type="vertbox" hsp="expand" vsp="expand" hap="left" vertical_gutter="3" vertical_padding="5" horizontal_padding="10" name="fundamentalsVertBox2">
				<container type="vertbox" hsp="expand" vsp="expand" hap="center" vertical_gutter="3" vertical_padding="5" solidfill="1" solidfillname="miniPaperBrassLight" >
